{
    "name": "mcp-tcp-bridge",
    "version": "1.0.0",
    "description": "将STDIO型MCP服务器桥接到TCP端口",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc && npm run bundle && npm run copy",
        "bundle": "ncc build dist/index.js -o bundle && ncc build dist/spawn-helper.js -o bundle-helper",
        "copy": "node -e \"require('fs').copyFileSync('bundle/index.js', '../../app/src/main/assets/bridge/index.js')\" && node -e \"require('fs').copyFileSync('bundle-helper/index.js', '../../app/src/main/assets/bridge/spawn-helper.js')\"",
        "start": "node dist/index.js",
        "dev": "ts-node index.ts",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "keywords": [
        "mcp",
        "bridge",
        "tcp",
        "stdio"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "uuid": "^9.0.0",
        "mcp-client": "^1.13.1",
        "@modelcontextprotocol/sdk": "^1.18.2"
    },
    "devDependencies": {
        "@types/node": "^18.0.0",
        "@types/uuid": "^9.0.1",
        "@vercel/ncc": "^0.38.1",
        "ts-node": "^10.9.1",
        "typescript": "^5.0.0"
    }
}